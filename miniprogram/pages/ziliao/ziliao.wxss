.page {
    display: flex;
    flex-direction: column;
    align-items: center;
    margin-top: 20px;
}

.top {
    width: 200rpx;
    height: 200rpx;
    border-radius: 100rpx;
    overflow: hidden;
}

.center {
    margin-top: 100rpx;
    width: 100%;
    display: flex;
    flex-direction: row;
    justify-content: center;
}

.bottom {
    margin-top: 100rpx;
    width: 100%;
    display: flex;
    flex-direction: row;
    justify-content: center;
}